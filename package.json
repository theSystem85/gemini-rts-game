{
  "name": "vite-starter",
  "private": true,
  "version": "0.15.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "stun": "node server/stun.js",
    "prebuild": "node scripts/write-commit.js",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/ tests/ *.js",
    "lint:fix": "eslint src/ tests/ *.js --fix",
    "map:analyse": "node map_analyse.js",
    "make:sprites": "node makeSprites.js",
    "count:locs": "node countLocs.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.32.0",
    "jsdom": "^27.4.0",
    "vite": "^6.0.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@netlify/blobs": "^10.4.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "serverless-http": "^4.0.0",
    "sharp": "^0.34.3"
  }
